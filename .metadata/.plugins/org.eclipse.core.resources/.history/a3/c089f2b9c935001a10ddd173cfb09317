import java.util.Scanner;
public class prime {
	public static void main(String[] args) {
		Scanner in =new Scanner(System.in);
		int i =0;
	      int num =0;
	      int n = in.nextInt();
	      for (i = 2; i <= n; i++)  	   
	      { 		 		  
	         int counter=0; 		  
	         for(num =i; num>1; num--)
	         {
	        	 if(i%num==0)
	        	 {
	        		 counter = counter + 1;
	        	 }
	         }
	         if (counter ==0)
	         {
	        	 System.out.println(i);
	         }	
	      }	
	}
}
